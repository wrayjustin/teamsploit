MSFCON=`whereis msfconsole | awk {' print $2 '}`
MSFCONPATH=`readlink $MSFCON | sed -e 's/\/app\/msfconsole//'`
MSF3DIR=`find $MSFCONPATH -type d -name "msf3" -o -type d -name "msf3" 2>/dev/null`
MSF3SYM=`readlink $MSF3DIR`

if [[ $MSF3SYM ]]; then
	echo $MSF3SYM
else
	echo $MSF3DIR | sed -e 's/\/msf3$//'
fi
